#password

n=input("Enter number")
up,dg,sp,lp=0,0,0,0
if len(n)>7:
    for i in n:
        if i.islower():
            lp+=1
        elif i.isupper:
            up+=1
        elif i.isdigit():
            dg+=1
        else:
            sp+=1
    if up>0 and dg>0 and sp>0 and lp>0:
        print("Good password")
    else:
        print("Bad password")
else:
    print("enter the password of length above 7")
